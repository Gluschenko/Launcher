<Window x:Class="Launcher.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="400" MinWidth="400" Height="400" Width="400">
    <Grid>
        <Grid Height="120" VerticalAlignment="Top">
            <Image Stretch="UniformToFill"/>
            <Label Content="Version" HorizontalAlignment="Right" VerticalAlignment="Bottom" Foreground="White">
                <Label.Effect>
                    <DropShadowEffect BlurRadius="3" Opacity="0.5" ShadowDepth="2"/>
                </Label.Effect>
            </Label>
        </Grid>
        <Grid Margin="0,120,0,0">
            <StackPanel Height="30" VerticalAlignment="Top" Orientation="Horizontal">
                <Button Content="1234"></Button>
                <Button Content="1234"></Button>
                <Button Content="1234"></Button>
            </StackPanel>
            <Frame x:Name="FrameView" Margin="0,30,0,0" NavigationUIVisibility="Hidden"></Frame>
        </Grid>
    </Grid>
</Window>
